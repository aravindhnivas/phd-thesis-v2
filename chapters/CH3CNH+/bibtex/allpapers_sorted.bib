@Article{FGA2016,
  author    = {P. Fathi and W.D. Geppert and D. Ascenzi},
  title     = {Experimental and theoretical investigations of the ion-neutral reaction of C2H2N$\mathplus$ with C2H6 and implications on chain elongation processes in Titan{\textquotesingle}s atmosphere},
