{EEL2017,
  author   = {E.S. Endres and G. Egger and S. Lee and O. Lakhmanskaya and M. Simpson and R. Wester},
  title    = {Incomplete rotational cooling in a 22-pole ion trap},
  journal  = {J. Mol. Spectrosc.},
  year     = {2017},
  volume   = {332},
  pages    = {134 - 138},
  issn     = {0022-2852},
  note     = {Molecular Spectroscopy in Traps},
  abstract = {Cryogenic 22-pole ion traps have found many applications in ion-molecule reaction kinetics and in high resolution molecular spectroscopy. For most of these applications it is important to know the translational and internal temperatures of the trapped ions. Here, we present detailed rotational state thermometry measurements over an extended temperature range for hydroxyl anions in He, HD, and H2. The measured rotational temperatures show a termination of the thermalisation with the buffer gas around 25K, independent of mass ratio and confinement potential of the trap. Different possible explanations for this incomplete thermalisation are discussed, among them the thermalisation of the buffer gas, room temperature blackbody radiation or warm gas entering the trap, and heating due to energy transfer from rotationally excited hydrogen molecules.},
  doi      = {https://doi.org/10.1016/j.jms.2016.12.006},
  keywords = {Photodetachment spectroscopy, Multipole ion trap, Buffer gas cooling, Rotational temperatures},
  //url      = {http://www.sciencedirect.com/science/article/pii/S0022285216303629},
}



