@Article{TJN2020,
author ="Töpfer, Matthias and Jensen, Anders and Nagamori, Keigo and Kohguchi, Hiroshi and Szidarovszky, Tamás and Császár, Attila G. and Schlemmer, Stephan and Asvany, Oskar",
title  ="Spectroscopic signatures of HHe2+ and HHe3+",
journal  ="Phys. Chem. Chem. Phys.",
year  ="2020",
volume  ="22",
issue  ="40",
pages  ="22885-22888",
publisher  ="The Royal Society of Chemistry",
doi  ="10.1039/D0CP04649C",
//url  ="http://dx.doi.org/10.1039/D0CP04649C",
abstract  ="Using two different action spectroscopic techniques{,} a high-resolution quantum cascade laser operating around 1300 cm−1 and a cryogenic ion trap machine{,} the proton shuttle motion of the cations HHe2+ and HHe3+ has been probed at a nominal temperature of 4 K. For HHe3+{,} the loosely bound character of this complex allowed predissociation spectroscopy to be used{,} and the observed broad features point to a lifetime of a few ps in the vibrationally excited state. For He–H+–He{,} a fundamental linear molecule consisting of only three nuclei and four electrons{,} the method of laser-induced inhibition of complex growth (LIICG) enabled the measurement of three accurate rovibrational transitions{,} pinning down its molecular parameters for the first time."}

