  journaltitle = {{Rapid Commun. Mass. Spectrom.}},
  number       = {{13}},
  pages        = {{1110--1115}},
  title        = {{Dissociation dynamics of propargyl chloride molecular ion near the reaction threshold: manifestation of quantum mechanical tunneling}},
