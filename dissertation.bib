  shorttitle   = {{COLTRAP}},
  url          = {https://doi.org/10.1007/s00340-013-5684-y},
  urldate      = {2022-09-29}
}

@article{asvany_experimental_2005,
  abstract     = {The spectrum of the ν5 cis-bending vibration of ground state (X2Πu) C2H+2 has been recorded applying the method of laser induced reactions in a low-temperature 22-pole ion trap. It is obtained by counting the number of products of the reaction C2H+2 (v5=1) +H2→C2H+3+H as a function of the laser wavelength. The vibronic transitions Δ−Π and Σ−Π with their corresponding spin-orbit and Renner-Teller substructure have been observed. Using a perturbative analysis, the vibrational frequency has been determined to ω5=(710±4) cm−1 and the Renner-Teller parameter ϵ5 is on the order of 3×10−2.},
