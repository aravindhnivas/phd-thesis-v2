@article{plasil_stabilization_2012,
  abstract     = {Formation of via association of H+ with H2 has been studied at low temperatures using a 22-pole radiofrequency trap. Operating at hydrogen number densities from 1011 to 1014 cm−3, the contributions of radiative, kr, and ternary, k3, association have been extracted from the measured apparent binary rate coefficients, k*=kr+k3[H2]. Surprisingly, k3 is constant between 11 and 22 K, (2.6±0.8)×10−29 cm6 s−1, while radiative association decreases from kr(11 K)=(1.6±0.3)×10−16 cm3 s−1 to kr(28 K)=(5±2)×10−17 cm3 s−1. These results are in conflict with simple association models in which formation and stabilization of the complex are treated separately. Tentative explanations are based on the fact that, at low temperatures, only few partial waves contribute to the formation of the collision complex and that ternary association with H2 may be quite inefficient because of the ‘shared proton’ structure of .},
  author       = {Plašil, Radek and Zymak, Illia and Jusko, Pavol and Mulin, Dmytro and Gerlich, Dieter and Glosík, Juraj},
  url          = {https://royalsocietypublishing.org/doi/10.1098/rsta.2012.0098},
