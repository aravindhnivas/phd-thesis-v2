@article{bates_density_1951,
  abstract     = {Analysis of Adams' visual estimates indicates that the interstellar lines {CH}+ x 4232 and {CH} x 4300 originate in two types of regions-in the normal obscuring clouds responsible for selective extinction and in circumstellar clouds near stars of late B types. In stars of types 0 and early B, the intensities of X 4232 and N 4300 are closely correlated with color excess, E1 the mean particle densities of these molecules, averaged over the volume in and between the clouds, is about 10-8 . Among stars of later B types, many unreddened stars (E1 0.05) show strong molecular lines; the number of molecules per square centimeter in the line of sight averages about 3 X 10 cm for types B2-B5. Of fifteen unreddened stars showing the molecular lines, the velocity difference, interstellar minus stellar lines, is positive in all but one case. A theoretical analysis shows that many of the rate coefficients affecting the equilibrium abundance of {CH} and {CH}+ molecules have different values from those assumed hitherto. In particular, the formation of {CH} by radiative association is markedly slower than previously believed, and there is some possibility that dissociative recombination ({CH}+ + e H C + H) is very rapid. If best estimates are adopted for all the rate coefficients, the observed molecular densities can be explained only if the clouds are extremely concentrated, with a central density no(H) of neutral H about 10 . If, however, the f-value for {CH} N 4300 is increased by a factor of 30 above the theoretical figure, an increase for which there is some observational evidence, agreement is obtained with no(H) equal to about 80. Mternatively, the molecules may be formed at the surface of the grains. The molecules observed in circumstellar clouds may originate from the sublimation of {CH}4 from the grains, a process which should occur when the internal temperature of the grains rises much above some 25 K. The {CH}4 molecules will dissociate, forming a hemispherical shell of {CH}+ molecules on the side of the star from which the cloud is advancing. This hypothesis explains why the circumstellar molecular lines in an unreddened star show a greater radial velocity than do the stellar lines, and quantitative agreement with the number of observed {CH}+ molecules is obtained with reasonable values for the unfortunately rather uncertain physical parameters. However, once again, agreement with the observed abundance of {CH} can be obtained only if the f-value for N 4300 is taken to be 30 times that indicated by quantal calculation.},
  author       = {Bates, David R. and Spitzer, Jr., Lyman},
  url          = {https://ui.adsabs.harvard.edu/abs/1951ApJ...113..441B},
  date         = {1951-05-01},
  doi          = {10.1086/145415},
  issn         = {0004-637X},
  journaltitle = {The Astrophysical Journal},
  note         = {{ADS} Bibcode: 1951ApJ...113..441B},
  pages        = {441},
  title        = {The Density of Molecules in Interstellar Space.},
  urldate      = {2022-09-20},
  volume       = {113}
}

