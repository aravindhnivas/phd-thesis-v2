  date         = {2016-09-01},
  doi          = {10.1051/0004-6361/201629101},
  issn         = {0004-6361, 1432-0746},
  journaltitle = {Astronomy \& Astrophysics},
  langid       = {english},
  note         = {Publisher: {EDP} Sciences},
  pages        = {A56},
  shortjournal = {A\&A},
