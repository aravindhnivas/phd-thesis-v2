  journaltitle = {Physical Review A},
  note         = {Publisher: American Physical Society},
  number       = {3},
  pages        = {032513},
  shortjournal = {Phys. Rev. A},
  title        = {Terahertz-visible two-photon rotational spectroscopy of cold \$\{{\textbackslash}mathrm\{{OD}\}\}{\textasciicircum}\{{\textbackslash}ensuremath\{-\}\}\$},
  urldate      = {2022-09-28},
  volume       = {93}
}

