  issn         = {15205126},
  journaltitle = {Journal of the American Chemical Society},
  number       = {15},
  pages        = {4378--4386},
  title        = {{Vibrational Spectra of C3H3+, C3D3+, C3H2D+, and C3D2H+ and Force Constants for this Ion System}},
  volume       = {108}
}

